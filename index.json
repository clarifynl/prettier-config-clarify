{
	"$schema": "http://json.schemastore.org/prettierrc",
	"semi": false,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"quoteProps": "consistent",
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"jsxSingleQuote": false,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"proseWrap": "preserve",
	"endOfLine": "lf"
}